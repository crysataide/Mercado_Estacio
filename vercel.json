{
    "version": 2,
    "rewrites": [
        { "source": "/", "destination": "/index.php" }
    ],
    "redirects": [
        { "source": "/", "destination": "/index.php", "permanent": true }
    ],
    "routes": [
        { "src": "/", "dest": "/index.php" }
    ],
    "builds": [
        { "src": "*.php", "use": "vercel-php" }
    ]
}
